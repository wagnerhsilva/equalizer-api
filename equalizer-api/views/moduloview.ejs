                                $("#alarme_tempMax").val("50.0");
                                $scope.alarmes[0].alarme_nivel_temp_max = "30.0";
                                // Impedancia
                                $("#alarme_impMin").val("1.0");
                                $scope.alarmes[0].alarme_nivel_imped_min = "0.1";
                                $("#alarme_impPreMax").val("80");
                                $scope.alarmes[0].alarme_nivel_imped_pre_max = "25.0";
